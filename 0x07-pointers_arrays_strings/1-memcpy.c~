#include"main.h"
/**
 *_memcpy - A function that copies memory area.
 *
 *@dest: pointer to destination area.
 *@src: pointer to source area.
 *@n: the number of bytes to be copied from @src memory 
 *area to memory area of @dest.
 *Return:value of new character string.
 */
char *_memcpy(char *dest, char *src, unsigned int n)
{
unsigned int i = 0;
while (i < n)
{
dest[i] = src[i];
i++;
}
return (dest);
}
