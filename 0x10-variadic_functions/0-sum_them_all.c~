#include<stdarg.h>
#include<stdio.h>
/**
 * sum_them_all - a function that sums all its parameters
 * @n: first argument
 *
 * Return: sum as an integer
 */
int sum_them_all(const unsigned int n, ...)
{
  if (n == 0)
      return (0);
  va_list tr;
  va_start(tr, n);
  int sum;
unsigned  int x;
  for (x = 0; x < n; x++)
    {
      int next = va_arg(tr, int);
      sum += next;
    }
  va_end(tr);
  return (sum);
}
